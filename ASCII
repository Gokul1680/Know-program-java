import java.util.Scanner;

public class ASCII {

    public static void main(String[] args) {
        String x;
        int ascii;
        Scanner scan = new Scanner(System.in);

        x = scan.nextLine();
        for (int i = 0 ; i < x.length() ; i++){
            ascii = (int) x.charAt(i);
            System.out.println("ASCII value of " + x.charAt(i) + " is " + ascii);
        }


        scan.close();
    }

}
